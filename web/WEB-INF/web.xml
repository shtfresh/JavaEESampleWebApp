<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
  
 

    <servlet>
        <servlet-name>AddInfomation</servlet-name>
        <servlet-class>oracle.com.servlet.AddInfomation</servlet-class>
    </servlet>
  
    <servlet>
        <servlet-name>ShowInfomationList</servlet-name>
        <servlet-class>oracle.com.servlet.ShowInfomationList</servlet-class>
    </servlet>
    

    <servlet-mapping>
        <servlet-name>ShowInfomationList</servlet-name>
        <url-pattern>/show</url-pattern>
    </servlet-mapping>
  
    <servlet-mapping>
        <servlet-name>AddInfomation</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    

    
</web-app>
